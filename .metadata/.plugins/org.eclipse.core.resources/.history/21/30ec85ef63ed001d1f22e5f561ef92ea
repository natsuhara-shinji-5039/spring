package com.example.demo.model;

import org.springframework.stereotype.Component;

@Component
@Session
public class Account {
	private String name;
	
	public Account(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
}
